{
    "version": 2,
    "builds": [
        {
            "src": "frontend-opendata/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        },
        {
            "src": "backend-opendata/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/backend/(.*)",
            "dest": "/backend-opendata/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend-opendata/$1"
        }
    ]
}